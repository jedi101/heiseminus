{
  "name": "HeiseMinus",
  "version": "0.0.9",
  "description": "Entfernt automatisch Heise Plus Artikel",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.heise.de/*"
      ],
      "exclude_matches": [
        "https://*.heise.de/plus/*"
      ],
      "run_at": "document_end",
      "js": [
        "config.js",
        "tag.js",
        "heiseminus.js",
        "main.js"
      ]
    }
  ],
  "icons": {
    "16": "images/heiseminusIcon16px.png",
    "48": "images/heiseminusIcon48px.png",
    "128": "images/heiseminusIcon128px.png"
  },
  "web_accessible_resources": [
    "images/*.svg"
  ],
  "manifest_version": 2
}